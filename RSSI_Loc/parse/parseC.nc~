  uses interface PacketField<uint8_t> as PacketRSSI;
  
  provides interface Intercept as RadioParse;
}

implementation {
  default event bool RadioParse.forward[am_id_t amid](message_t* msg, void* payload, unit8_t len){
    return TURE;
  }
}